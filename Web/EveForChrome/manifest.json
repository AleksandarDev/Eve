{
	"name": "Eve for Chrome",
	"description": "__MSG_extensionDescription__",
	"version": "0.1.0.0",
	"manifest_version": 2,
	"default_locale": "en",
	"homepage_url": "https://eve.toplek.net/",
	"icons": {
		"128": "Resources/Icons/Eve128.png",
		"48": "Resources/Icons/Eve48.png"
	},
	"permissions": [ 
		"notifications",
		"storage",
		"unlimitedStorage",
		"tabs",
		"*://*/"
	],
	"background": {
		"page": "Pages/Background/Background.html",
		"persistent": true
	},
	"browser_action": {
		"default_icon": "Resources/Icons/Eve48.png",
		"default_title": "Eve for Chrome",
		"default_popup": "Pages/Popup/Popup.html"
	},
	"options_page": "Pages/Options/Options.html",
	"web_accessible_resources": [	]
}
